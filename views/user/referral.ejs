<%- include('../layouts/user/userHeader.ejs') %>
    <!-- Spinner Start -->
    <div id="spinner"
        class="show bg-white position-fixed translate-middle w-100 vh-100 top-50 start-50 d-flex align-items-center justify-content-center">
        <div class="spinner-grow text-primary" style="width: 3rem; height: 3rem;" role="status">
            <span class="sr-only">Loading...</span>
        </div>
    </div>
    <!-- Spinner End -->


    <!-- Navbar Start -->
    <%- include('../layouts/user/userNavbar.ejs') %>
        <!-- Navbar End -->

        <!-- Hero Start -->
        <div class="container-fluid bg-primary hero-header mb-5">
            <div class="container text-center">
                <h1 class="display-4 text-white mb-3 animated slideInDown">Referral Link</h1>
            </div>
        </div>
        <!-- Hero End -->

        <!-- referral start -->
        <style>
            body {
                background: #dedede;
            }

            .coupon .kanan {
                border-left: 1px dashed #ddd;
                width: 40% !important;
                position: relative;
            }

            .coupon .kanan .info::after,
            .coupon .kanan .info::before {
                content: '';
                position: absolute;
                width: 20px;
                height: 20px;
                background: #dedede;
                border-radius: 100%;
            }

            .coupon .kanan .info::before {
                top: -10px;
                left: -10px;
            }

            .coupon .kanan .info::after {
                bottom: -10px;
                left: -10px;
            }

            .coupon .time {
                font-size: 1.6rem;
            }
        </style>
        <div class="container my-5">
            <div class="row">
                <div class="col-sm-6 d-flex justify-content-center">
                    <div class="coupon bg-white rounded mb-3 d-flex justify-content-center">
                        <div class="kiri p-3">
                            <div class="icon-container ">
                                <div class="icon-container_box">
                                    <img src="data:image/png;base64,"
                                        width="85" alt="totoprayogo.com" class="" />
                                </div>
                            </div>
                        </div>
                        <div class="tengah py-3 d-flex w-150 justify-content-start">
                            <div>
                                <h3 class="lead"><b>Refer a friend and get ₹ 200</b></h3>
                                <p class="text-muted mb-0">Share this link to a friend and you both will get ₹ 200 in wallet after successful signup</p>
                                <p>http://localhost:3000/register?referralCode=<%= referral %></p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- referral end -->

        <!-- Newsletter Start -->
        <div class="container-fluid newsletter bg-primary py-5 my-5">
            <div class="container py-5">
                <div class="mx-auto text-center wow fadeIn" data-wow-delay="0.1s" style="max-width: 600px;">
                    <h1 class="text-white mb-3"><span class="fw-light text-dark">Let's Subscribe</span> The Newsletter
                    </h1>
                    <p class="text-white mb-4">Subscribe now to get 15% discount on any of our products</p>
                </div>
                <div class="row justify-content-center">
                    <div class="col-md-7 wow fadeIn" data-wow-delay="0.5s">
                        <div class="position-relative w-100 mt-3 mb-2">
                            <input class="form-control w-100 py-4 ps-4 pe-5" type="text" placeholder="Enter Your Email"
                                style="height: 48px;">
                            <button type="button" class="btn shadow-none position-absolute top-0 end-0 mt-1 me-2"><i
                                    class="fa fa-paper-plane text-white fs-4"></i></button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- Newsletter End -->


        <!-- Footer Start -->
        <%- include('../layouts/user/userFooter.ejs') %>